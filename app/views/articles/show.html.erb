<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <p><%= @article.title %></p>
  <% if @article.article_image.attached? %>
    <span>article image attached</span>
    <%= image_tag(@article.article_image) %>
  <% else %>
    <span>No article image attached</span>
  <% end %>
</p>

<%= link_to 'Edit', edit_article_path(@article) %> |
